function Main0()
SN = gg.choice({ --单选框
"粑粑123级冷却",  --功能名称
"退出脚本",
}, nil, "功能：修改1.2.3级粑粑的冷却") --标题
if SN==1 then HS1() end  
if SN==2 then os.exit() end
FX1=0
end

function HS1()
  gg.toast("搜索0条数据.修改失败") 
end

while true do
if gg.isVisible(true) then
    FX1 = nil
    gg.setVisible(false)
  end
  if FX1 == nil then
    Main0()
  end
end
